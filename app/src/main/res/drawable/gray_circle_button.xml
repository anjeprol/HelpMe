<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#6a6b6a" />
            <gradient android:angle="-90" android:startColor="#4d514d" android:endColor="#c4c6c4"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#565b57" />
            <solid android:color="#9ea39f"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#848684" />
            <gradient android:angle="-90" android:startColor="#747875" android:endColor="#555c56" />
        </shape>
    </item>
</selector>