<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#306c25" />
            <gradient android:angle="-90" android:startColor="#06ab0b" android:endColor="#56cc50"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#2ec94a" />
            <solid android:color="#7dd69b"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#7dda7d" />
            <gradient android:angle="-90" android:startColor="#7dd69b" android:endColor="#2ec94a" />
        </shape>
    </item>
</selector>